<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToggleView">
        <attr name="backgroundOff" format="reference|color" />
        <attr name="backgroundOn" format="reference|color" />
        <attr name="initOn" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PopupMenu">
        <attr name="popupBottom" format="reference" />
        <attr name="popupContent" format="reference" />
        <attr name="animStyle" format="reference" />
        <attr name="popupOffsetX" format="dimension" />
        <attr name="popupOffsetY" format="dimension" />
        <attr name="overlay" format="dimension" />
        <attr name="padding" format="dimension" />
    </declare-styleable>

</resources>